<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <appSettings>
	  <!-- SMTP Settings -->
	  <add key="SMTPHost" value="smtp.office365.com" />
	  <add key="SMTPPort" value="587" />
	  <add key="SMTPUser" value="NoReply@Example.com" />
	  <add key="SMTPPass" value="" />

	  <!-- Basic Time Logger -->
      <add key="BasicTimeLogger" value="true" />
      <add key="BasicTimeLoggerFrequency" value="1"/>

      <!-- Email Test Logger -->
      <add key="EmailTest" value="false"/>
      <add key="EmailTestFrequency" value="1"/>
	  <add key="EmailTestMailTo" value="Test@Example.com"/>
	  
	  <!-- WriteToFile -->
	  <add key="WriteToFile" value="false"/>
	  <add key="WriteToFileFrequency" value="5"/>
	  <add key="WriteToFileDir" value="C:\Files\Test"/>

  </appSettings>
</configuration>